<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xs:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
               xmlns:annox="http://annox.dev.java.net" jaxb:extensionBindingPrefixes="xjc annox" jaxb:version="2.0">
    <jaxb:bindings schemaLocation="../mapv2.xsd" node="/xs:schema">
        <jaxb:globalBindings generateMixedExtensions="true">
            <!-- xjc:serializable will cause the XJC binding compiler to generate classes that implement java.io.Serializable. -->
            <xjc:serializable uid="1" />
            <xjc:javaType name="java.lang.Double" xmlType="xs:decimal" adapter="be.nevies.game.engine.tiled.plugin.jaxb.DecimalAdapter" />
            <xjc:javaType name="java.lang.Long" xmlType="xs:integer" adapter="be.nevies.game.engine.tiled.plugin.jaxb.IntegerAdapter"  />
        </jaxb:globalBindings>
        
        <jaxb:schemaBindings>
            <jaxb:package name="be.nevies.game.engine.tiled.plugin.map" />
<!--            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="http://mapeditor.org" />
            </jaxb:nameXmlTransform>-->
        </jaxb:schemaBindings>        
    </jaxb:bindings>

</jaxb:bindings>